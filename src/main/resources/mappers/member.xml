<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.b01.mapper.MemberMapper">
    <!-- 회원 등록 -->
    <insert id="insert">
        insert into
            member (mid, mname, mpw, uuid)
        values
            (#{mid}, #{mname}, #{mpw}, #{uuid})
    </insert>
</mapper>